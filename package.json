    "request-promise": "^2.0.1",
    "sprintf": "^0.1.5"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "istanbul": "^0.4.3",
    "mathjs": "^3.3.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "nock": "^7.7.2",
    "sinon": "^1.17.4"
  }
}
